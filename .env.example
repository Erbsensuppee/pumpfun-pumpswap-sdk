# Example .env file for pumpfun-pumpswap-sdk
# Copy this file to `.env` and fill in the values below.
# DO NOT commit your actual .env file to version control (add it to .gitignore).

# Helius RPC endpoint for Solana (get one at https://www.helius.dev)
HELIUS_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_API_KEY_HERE

# Your wallet's private key (base58-encoded)
# WARNING: Keep this secret! Never share or commit it.
PRIVATE_KEY=YourBase58PrivateKeyHere

# Optional: buy mode used by `npm run test:buy`
# Valid values: buy | buy_exact_sol_in
BUY_MODE=buy

# Optional: whether to set track_volume=true in Pump.fun bonding-curve buy tests
# Set to false to isolate cashback/volume accumulator issues
TRACK_VOLUME=true

# Optional: if BUY_MODE=buy, retry with buy_exact_sol_in on overflow
BUY_FALLBACK_EXACT_SOL_IN=false

# Optional: extra remaining account appended to Pump.fun bonding-curve buy instruction.
# Some coins/paths use one trailing remaining account beyond the base IDL account list.
PUMPFUN_BUY_REMAINING_ACCOUNT=

# Optional: mark the extra Pump.fun buy remaining account writable (default false)
PUMPFUN_BUY_REMAINING_ACCOUNT_WRITABLE=false
